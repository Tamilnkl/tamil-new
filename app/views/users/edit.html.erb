<h2>Edit User<h2> <%= form_for(@user) do |f|%>
<p>
	<%= f.label :name %>
	
	<%= f.text_field :name %>
</p>
<p>
	<%= f.label :email %>
	
	<%= f.text_field :email %>
</p>
<p>
	<%= f.label :user_type %>
	
	<%= f.text_field :user_type %>
</p>
<p>
	<%= f.label :password %>
	
	<%= f.password_field :password %>
	<p>
    <%= f.submit %>
  </p>
</p> <% end %>

<%= link_to 'Show', @user %> |
<%= link_to 'Back', user_path %>